<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AssemblyName>DotNetCore.Repositories</AssemblyName>
        <Description>The package provides interfaces for generic repositories.</Description>
        <PackageId>DotNetCore.Repositories</PackageId>
        <PackageProjectUrl>https://github.com/rafaelfgx/DotNetCore/tree/master/source/Repositories</PackageProjectUrl>
        <PackageTags>Repository, GenericRepository, RepositoryBase, CommandRepository, QueryRepository, Specification, Pattern</PackageTags>
